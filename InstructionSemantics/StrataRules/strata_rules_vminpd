(vminpd xmm, xmm, m128) -> (OP1 = 0x0_128 o ((mincmp_double(OP2[127:64], OP3[127:64]) == 0x1_1 ? OP2[127:64] : OP3[127:64]) o (mincmp_double(OP2[63:0], OP3[63:0]) == 0x1_1 ? OP2[63:0] : OP3[63:0])))
