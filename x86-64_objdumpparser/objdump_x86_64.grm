%name x86_64
%noshift EOF
%nodefault
%nonassoc BYTEPTR WORDPTR DWORDPTR QWORDPTR
%left PLUS MINUS
%left MULT
%%

START:
	  STARTT ((let val _ = section_number := 0 in STARTT end))

STARTT:
	  STARTT DATA_SECTION (add_data_section_to_assembly DATA_SECTION STARTT)
	| STARTT TEXT_SECTION (add_text_section_to_assembly TEXT_SECTION STARTT)
	| (empty_assembly)

TEXT_SECTION:
	DOTTEXT COLON CODESEGMENT TEXT_LINES (build_text_section (filter (fn dl => dl <> Text_Empty) TEXT_LINES))

(* Eventually .rodata and .data sections should be passed up differently to disginguish them. .rodata is supposed to be read only. *)
DATA_SECTION:
    DOTDATA COLON DATASEGMENT DATA_LINES (build_data_section (filter (fn dl => dl <> Data_Empty) DATA_LINES))
  | DOTRODATA COLON DATASEGMENT DATA_LINES (build_data_section (filter (fn dl => dl <> Data_Empty) DATA_LINES))

TEXT_LINES:
	  TEXT_LINE NEWLINE TEXT_LINES (TEXT_LINE::TEXT_LINES)
	| NEWLINE TEXT_LINES (TEXT_LINES)
	| ([])

DATA_LINES:
	  DATA_LINE NEWLINE DATA_LINES (DATA_LINE::DATA_LINES)
	| NEWLINE DATA_LINES (DATA_LINES)
	| ([])

	
NATNUM:
	HEXPREFIX NAT (NAT)
	| NAT (NAT)

	
ADDL_INSTRUCTION_INFO:
  HASHY NATNUM (NATNUM)

DATA_LINE:
	NATNUM ADDL_INSTRUCTION_INFO (Data_Data (Data_Constant (NATNUM,1)))


TEXT_LINE:
	  INSTR_MNEMONIC ADDL_INSTRUCTION_INFO (Text_Instr (Nullary INSTR_MNEMONIC,ADDL_INSTRUCTION_INFO))
	| INSTR_MNEMONIC OPERAND_SRC ADDL_INSTRUCTION_INFO (Text_Instr (Unary (INSTR_MNEMONIC,OPERAND_SRC),ADDL_INSTRUCTION_INFO))
	| INSTR_MNEMONIC OPERAND_DEST COMMA OPERAND_SRC ADDL_INSTRUCTION_INFO (Text_Instr (Binary (INSTR_MNEMONIC,OPERAND_DEST,OPERAND_SRC),ADDL_INSTRUCTION_INFO))
	| INSTR_MNEMONIC OPERAND_DEST COMMA OPERAND_SRC COMMA OPERAND_SRC ADDL_INSTRUCTION_INFO (Text_Instr (Ternary (INSTR_MNEMONIC,OPERAND_DEST,OPERAND_SRC1,OPERAND_SRC1),ADDL_INSTRUCTION_INFO))

IMMEDIATE:
      MINUS NATNUM (ImmSixtyFour (~ NATNUM))
	| NATNUM (ImmSixtyFour NATNUM)

OPERAND_DEST:
	  REG %prec REG_TERMINAL (Reg REG)
	| MEMORY %prec MEM_TERMINAL (Memory MEMORY)

OPERAND_SRC:
	  IMMEDIATE (Immediate IMMEDIATE)
	| OPERAND_DEST (Storage OPERAND_DEST)
(* BEGIN SIGNATURE NMENONICS *)

INSTR_PREFIX:
	REP (1)
	| REPE (1)
	| REPNE (1)
	| REPNZ (1)
	| REPZ (1)
	| LOCK (1)
	| DATA16 (1)
	| DATA16 DATA16 DATA16 (1)
INSTR_MNEMONIC:
	INSTR_MNEMONIC_SUB (INSTR_MNEMONIC_SUB)
	| INSTR_PREFIX INSTR_MNEMONIC_SUB (INSTR_MNEMONIC_SUB)
		
